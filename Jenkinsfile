pipeline {
    agent any

    stages {
        stage('Build Docker Image') {
            steps {
                sh 'echo "Executando o comando Docker build"'
            }
        }

        stage('Push Docker Image') {
            steps {
                sh 'echo "Executando o comando Docker build"'
            }
        }

         stage('Deploy Docker Image') {
            steps {
                sh 'echo "Executando o comando Docker build"'
            }
        }
    }
}